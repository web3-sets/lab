{
  "name": "nextra-theme-blog",
  "version": "2.4.1",
  "description": "A Nextra theme for blogs.",
  "repository": "https://github.com/shuding/nextra",
  "author": "Shu Ding <g@shud.in>",
  "license": "MIT",
  "files": [
    "dist/*",
    "style.css"
  ],
  "sideEffects": false,
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "cusdis": [
        "./dist/cusdis.d.ts"
      ],
      "tags": [
        "./dist/tags.d.ts"
      ]
    }
  },
  "exports": {
    "./style.css": "./style.css",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cusdis": {
      "import": "./dist/cusdis.js",
      "types": "./dist/cusdis.d.ts"
    },
    "./tags": {
      "import": "./dist/tags.js",
      "types": "./dist/tags.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:tailwind": "pnpm postcss src/styles.css -o style.css --verbose",
    "build:all": "pnpm build && pnpm build:tailwind",
    "types": "tsup --dts-only",
    "types:check": "tsc --noEmit",
    "dev": "concurrently \"pnpm dev:layout\" \"pnpm dev:tailwind\"",
    "dev:layout": "tsup --watch",
    "dev:tailwind": "TAILWIND_MODE=watch pnpm postcss src/styles.css -o style.css --watch",
    "prepublishOnly": "pnpm build:all",
    "clean": "rimraf ./dist ./style.css",
    "test": "vitest run",
    "format": "prettier --ignore-path ../../.gitignore --write --list-different ."
  },
  "dependencies": {
    "next-themes": "^0.2.1"
  },
  "peerDependencies": {
    "next": ">=9.5.3",
    "nextra": "workspace:*",
    "react": ">=16.13.1",
    "react-cusdis": "^2.1.3",
    "react-dom": ">=16.13.1"
  },
  "peerDependenciesMeta": {
    "react-cusdis": {
      "optional": true
    }
  },
  "devDependencies": {
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@tailwindcss/typography": "^0.5.8",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "concurrently": "^8.0.0",
    "next": "^13.1.1",
    "nextra": "workspace:*",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-lightningcss": "^0.7.0",
    "react": "^18.2.0",
    "react-cusdis": "^2.1.3",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.1",
    "vitest": "^0.30.0"
  }
}
